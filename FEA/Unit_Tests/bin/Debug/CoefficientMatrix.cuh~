template<typename T, int Size, int Dims>
struct CoefficientMatrix{
__device__ CoefficientMatrix() {}
T C[Size][Size];
__device__ void GetAlpha(T A[Size], const T U[Size]) {
	#pragma unroll
	for (int i = 0; i < Size; i++) {
		T Sum = 0;
		#pragma unroll
		for (int j = 0; j < Size; j++) {
			Sum = Sum + C[i][j] * U[j];
		}
		A[i] = Sum;
	}
}
};
